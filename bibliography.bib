@article{Cuomo2020,
  title = {Single-Cell {{RNA-sequencing}} of Differentiating {{iPS}} Cells Reveals Dynamic Genetic Effects on Gene Expression},
  author = {Cuomo, Anna S. E. and Seaton, Daniel D. and McCarthy, Davis J. and Martinez, Iker and Bonder, Marc Jan and {Garcia-Bernardo}, Jose and Amatya, Shradha and Madrigal, Pedro and Isaacson, Abigail and Buettner, Florian and Knights, Andrew and Natarajan, Kedar Nath and {HipSci Consortium} and Vallier, Ludovic and Marioni, John C. and Chhatriwala, Mariya and Stegle, Oliver},
